.borrow-txt i{position: absolute; left: 0; top:0; width:0.75rem; height: 0.75rem; background: url(../images/borrow-icon.png) no-repeat 0 -2.25rem; background-size: 2.9rem 3rem;}


/*借款记录 bichunfeng*/
.b-color-red{color:#e00000;}
.b-color-orange{color:#ff7e00;}
.b-withdraw-list{margin-top:0.75rem;padding:0.25rem 0.5rem;border-top:1px solid #e0e0e0;background-color: #fff;}
.b-withdraw-list>table{width:100%;table-layout: fixed;text-align: center;}
.b-withdraw-list th{height: 1.65rem;font-size: .75rem;border-right: 1px solid #e0e0e0;}
.b-withdraw-list td{padding:0.875rem 0;font-size:.65rem;}
.b-nopad{padding:0 !important;}
.b-nobr{border-right: none !important;}
.b-line-mart{margin-top:.25rem;}
.b-line{border-top:1px solid #e0e0e0;font-size: 0;margin-left:.7rem;}
.b-td-check{width:10%;}
.b-btn-check{width: 0.55rem;height: 1rem;display: inline-block;margin-left: 0.35rem;background: url("../images/m-gticon.png") 0 0 no-repeat;background-size: cover;}

/*授信管理 bichunfeng*/
.b-flex-box{display: -webkit-box;  display: -webkit-flex;  display: -ms-flexbox;  display: flex;  -webkit-box-align: center;  -webkit-align-items: center;  -ms-flex-align: center;  align-items: center;}
.b-credit-list{height: 3.25rem;border-bottom:1px solid #e0e0e0;font-size: 0.75rem;}
.b-credit-list:last-child{border: 0;}
.b-credit-info{margin:0.75rem;font-size:0.75rem;}
.b-credit-icon{padding-left:0.175rem;width:3rem;text-align: center;}
.b-icon{display:inline-block;width:1.75rem;height:1.75rem;line-height:1.95rem;border-radius: 50%;background-color:#f5f5f5;}
.b-icon1{width:0.7rem;}
.b-icon2{width:0.775rem;}
.b-icon3{width:0.75rem;}
.b-icon4{width:1.05rem;}
.b-icon5{width:1.275rem;}
.b-icon6,.b-icon7{width:0.8rem;}
.b-credit-txt{text-align: left;width: 60%;}
.b-credit-btnbox{width:4.75rem;text-align: center;}
.b-credit-btn{display: block;width:3.3rem;line-height: 1.35rem;font-size:.6rem;text-align:center;margin-right:0.75rem;color:#5d5d5d;border-radius: 0.1rem;}
.btn-default{background-color:#f1f1f1;}
.btn-active{background-color:#ff7200;color:#fff;}
.b-credit-marb{margin-bottom: 0.75rem;}

/*补充信息 bichunfeng*/
.b-supple-infotxt{ padding:0.75rem 0.75rem 0; font-size: 0.65rem;}
.b-form-select{position:relative;z-index:2;width: 100%;height: auto;font-size: 0.75rem;line-height: 1.5rem;color: #444444;font-family: "STHeiti","Microsoft YaHei","Microsoft YaHei","SimHei", "Arial Black";font-weight: lighter;background-color: transparent;}
.b-tip-bar{position:relative;padding:0 0.75rem;line-height:1.125rem;color:#666;background-color: #fbeabc;font-size:0.5rem; }
.t-tip-close{position:absolute;top:0.35rem;right:0.75rem;display: inline-block;width:0.4rem;height:0.4rem;background: url("../images/b-tip-close.png") center center no-repeat;font-size: 0;}
.b-supple-tiparea{margin:1.25rem 0;line-height:.75rem;text-align: center;color:#919191;font-size:0.6rem;}
